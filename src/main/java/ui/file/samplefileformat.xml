<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE circuit SYSTEM "circuit.dtd">
<circuit name="Sample File Format" createdOn="11 November 2008, 17:45" author="matt" 
        description="Possible format for saving a circuit layout in a file that can be loaded and saved ">
    <components>
        <component type="AndGate2Input" x="50" y="10" rotation="0"></component>
        <component type="NorGate2Input" x="50" y="80" rotation="0"></component>
        <component type="NandGate2Input" x="140" y="400" rotation="0"></component>
        <component type="inSource" x="10" y="10" rotation="0">
            <attr name="value" value="On" />
        </component>
        <component type="inSource" x="10" y="30" rotation="0">
            <attr name="value" value="Off" />
        </component>
        <component type="inSource" x="10" y="70" rotation="0">
            <attr name="value" value="Off" />
        </component>
        <component type="inSource" x="10" y="90" rotation="0">
            <attr name="value" value="On" />
        </component>
        <component type="LED" x="220" y="50" rotation="0">
            <attr name="color" value="yellow"/>
        </component>
        <wire startx="40" starty="20" endx="60" endy="20"></wire>     
        <wire startx="40" starty="40" endx="60" endy="40"></wire>     
        <wire startx="40" starty="80" endx="60" endy="80"></wire>     
        <wire startx="40" starty="100" endx="60" endy="100"></wire>     
        <wire startx="110" starty="30" endx="140" endy="50">
            <waypoint x="120" y="50"/>
        </wire>  
        <wire startx="110" starty="90" endx="140" endy="70">
            <waypoint x="120" y="90"/>
        </wire>   
        <wire startx="200" starty="60" endx="220" endy="60"></wire>
    </components>
</circuit>
